            console.log('No selection detected, trying delete all method...');
            
            // Delete all content
            const deleteAllEvent = new KeyboardEvent('keydown', {
              key: 'a',
              code: 'KeyA',
              ctrlKey: true,
              bubbles: true
            });
            document.dispatchEvent(deleteAllEvent);
            
